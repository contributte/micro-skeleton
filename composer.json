{
  "name": "planette/timy-project",
  "description": "Timy is experimental skeleton for really micro Nette projects by @f3l1x.",
  "keywords": [
    "php",
    "nette",
    "planette",
    "contributte",
    "sandbox",
    "project",
    "web"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "contributte/bootstrap": "^0.5.0",
    "contributte/di": "^0.5.0",
    "contributte/application": "^0.5.0",
    "contributte/forms": "^0.5.0",
    "contributte/http": "^0.4.0",
    "contributte/utils": "^0.5.0",
    "contributte/latte": "^0.5.0",
    "contributte/tracy": "^0.5.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.12",
    "ninjify/nunjuck": "^0.4",
    "contributte/dev": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "App\\Model\\": "app/model",
      "App\\Components\\": "app/components"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "platform": {
      "php": "7.4"
    }
  }
}
