{
  "name": "contributte/micro-skeleton",
  "description": "Micro project skeleton based on Nette Framework (@nette) and Contributte (@contributte) libraries for really small projects or sites by @f3l1x.",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "sandbox",
    "project",
    "skeleton",
    "web"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.0",

    "contributte/bootstrap": "^0.5.0",
    "contributte/di": "^0.5.0",
    "contributte/application": "^0.5.0",
    "contributte/forms": "^0.5.0",
    "contributte/http": "^0.4.0",
    "contributte/utils": "^0.5.0",
    "contributte/latte": "^0.5.0",
    "contributte/tracy": "^0.5.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.13",
    "ninjify/nunjuck": "^0.4",
    "contributte/dev": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "App\\Model\\": "app/model",
      "App\\Components\\": "app/components"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
